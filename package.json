{
  "name": "car-audio-competition-platform",
  "private": true,
  "version": "1.29.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:memory": "node --max-old-space-size=16384 --expose-gc --optimize-for-size --max-semi-space-size=512 --initial-old-space-size=4096 --no-concurrent-recompilation ./node_modules/vite/bin/vite.js",
    "build": "npm run version:generate && tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "analyze": "node scripts/analyze-bundle.js",
    "build:analyze": "npm run build && npm run analyze",
    "deploy:stripe": "node scripts/deploy-stripe-integration.js",
    "version:generate": "node scripts/generate-version.js",
    "version:check": "node scripts/generate-version.js && echo 'Version sync complete!'",
    "version:bump": "node scripts/bump-version.js",
    "version:bump:patch": "node scripts/bump-version.js patch",
    "version:bump:minor": "node scripts/bump-version.js minor",
    "version:bump:major": "node scripts/bump-version.js major",
    "mcp:start": "tsx src/mcp/server.ts",
    "mcp:build": "tsc src/mcp/server.ts --outDir dist/mcp --target es2022 --module esnext --moduleResolution node",
    "scan-secrets": "tsx scripts/scan-for-secrets.ts",
    "rotate-keys": "tsx scripts/rotate-keys.ts",
    "security:scan": "npm run scan-secrets",
    "security:maintain": "tsx scripts/security-maintenance.ts",
    "security:check": "npm audit && npm outdated",
    "security:auto-fix": "tsx scripts/security-maintenance.ts --auto-commit",
    "security:fix": "npm audit fix",
    "security:hooks": "pre-commit install",
    "security:audit": "npm audit --audit-level=moderate",
    "security:audit:fix": "npm audit fix",
    "security:outdated": "npm outdated",
    "security:update:check": "npm update --dry-run",
    "security:dependencies": "npm list --depth=0",
    "security:licenses": "npm ls --depth=0 --parseable | xargs npm view | grep license",
    "security:full-scan": "npm run security:audit && npm run scan-secrets && npm run security:outdated",
    "deps:update:safe": "npm update --save",
    "deps:update:dev": "npm update --save-dev",
    "deps:check:vulnerabilities": "npm audit --audit-level=high",
    "security:setup": "tsx scripts/setup-security.ts",
    "security:report": "tsx scripts/setup-security.ts && cat security-report.json",
    "type-check": "tsc --noEmit",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "security:update": "npm update"
  },
  "dependencies": {
    "@googlemaps/markerclusterer": "^2.6.2",
    "@hcaptcha/react-hcaptcha": "^1.12.1",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@paypal/react-paypal-js": "^8.8.3",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@stripe/react-stripe-js": "^3.9.0",
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/mcp-server-supabase": "^0.4.5",
    "@tinymce/tinymce-react": "^6.3.0",
    "@types/dompurify": "^3.0.5",
    "@types/jszip": "^3.4.0",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "dotenv": "^16.6.1",
    "framer-motion": "^12.23.12",
    "glob": "^11.0.3",
    "jszip": "^3.10.1",
    "lucide-react": "^0.541.0",
    "node-fetch": "^3.3.2",
    "openai": "^5.12.2",
    "pg": "^8.16.3",
    "quill": "^2.0.3",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-image-crop": "^11.0.10",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.30.1",
    "recharts": "^3.1.2",
    "supabase": "^2.33.9",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76"
  },
  "overrides": {
    "react-quill": {
      "quill": "^2.0.3"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@supabase/supabase-js": "^2.55.0",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^20.19.10",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/three": "^0.179.0",
    "@vitejs/plugin-legacy": "^6.1.1",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.15.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vite": "^6.3.5",
    "workbox-build": "^7.3.0",
    "workbox-window": "^7.3.0"
  }
}
